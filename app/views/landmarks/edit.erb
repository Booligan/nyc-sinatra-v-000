<h1>Edit <%= @landmark.name %></h1>

<form method="post" action="/landmarks/<%= @landmark.id %>">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label>Name:</label><input type="text" name="landmark[name]" id="name" value = <%= @landmark.name %>><br><br>
  <label>Year Completed:</label><input type="text" name="landmark[year_completed]" id="year_completed" value = <%= @landmark.year_completed.to_s %> ><br><br>
  <input type="submit" value="Edit Landmark">
</form>
